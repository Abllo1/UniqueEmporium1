{
  "compilerOptions": {
    "lib": ["deno.ns", "deno.window"],
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "bundler",
    "target": "es2022",
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true
  }
}